import pandas as pd

def save_res_to_csv(response, csv_filename="output.csv"):
    try:
        # Navigate into the nested res dict
        res_dict = response['output_result'][0]['res']

        # Convert dict â†’ DataFrame
        df = pd.DataFrame.from_dict(res_dict, orient="index")

        # Save to CSV
        df.to_csv(csv_filename, index=False)
        print(f"CSV saved successfully: {csv_filename}")
    except Exception as e:
        print(f"Error: {e}")

# Example usage
save_res_to_csv(response, "result_table.csv")
